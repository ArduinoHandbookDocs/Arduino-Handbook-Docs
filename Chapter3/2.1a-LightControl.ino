int ledPin= 13;

void setup(){
  pinMode(ledPin, OUTPUT); //pin13. OUTPUT
}

void loop(){
  digitalWrite(ledPin,HIGH); // output HIGH level
  signal to ledPin pin.
  delay(1000); //stop program 1000s(1 second)
  digitalWrite(ledPin, LOW); // output a LOW signal
  for ledPin pin
  delay(1000); //stop program 1000s(1 second)
}
